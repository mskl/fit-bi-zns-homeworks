from Graph.Graph import Graph


if __name__ == "__main__":
    g = Graph()
    g.add_edge(id_a="F", id_b="D")
    g.add_edge(id_a="A", id_b="D")
    g.add_edge(id_a="A", id_b="B")
    g.add_edge(id_a="C", id_b="B")
    g.add_edge(id_a="B", id_b="E")
    g.add_edge(id_a="D", id_b="E")

    print(g)
    g.remove_node("F")
    print(g)
